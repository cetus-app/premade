{
    "name": "promotion-centre",
    "tree": {
      "$className": "DataModel",
      "HttpService": {
        "$className": "HttpService",
        "$properties": {
          "HttpEnabled": true
        }
      },
      "ServerScriptService": {
        "$className": "ServerScriptService",
        "promotionServer": {
          "$path": "src/server",
          "dependencies": {
            "$className": "Folder",
            "cetus-rbx": {
              "$path": "../../cetus-rbx/src/cetus"
            }
          }
        }
      },
      "ServerStorage": {
        "$className": "ServerStorage",
        "Repository": {
          "$path": "src/Repository.rbxmx"
        }
      },
        "StarterPlayer": {
          "$className": "StarterPlayer",
          "StarterPlayerScripts": {
            "$className": "StarterPlayerScripts",
            "promotionClient": {
              "$className": "Folder",
              "$path": "src/client"
          }
        }
      },
      "ReplicatedStorage": {
        "$className": "ReplicatedStorage",
        "$ignoreUnknownInstances": true,
        "$path": "src/ReplicatedStorage"
      },
      "Workspace": {
        "$className": "Workspace",
        "$ignoreUnknownInstances": true,
        "stage": {
          "$path": "src/stage.rbxmx"
        }
      }
    }
  }